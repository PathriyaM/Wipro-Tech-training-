import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String n = sc.next();

        if (n.length() >= 2) {
            System.out.print(n.charAt(1) + "" + n.charAt(0));
        } else {
            System.out.print("STDOUT");
        }
    }
}
